{
    "name": "pinfort/laravel-topnav",
    "description": "Create simple top navigation for laravel",
    "type": "library",
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": ">=4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "pinfort",
            "email": "ptg@nijitei.com"
        }
    ],
    "keywords": ["laravel", "navigation", "menu"],
    "autoload": {
        "psr-4": {
            "Pinfort\\LaravelTopNav\\": "src/"
        }
	},
    "minimum-stability": "dev",
    "include-path": ["src/"],
    "extra": {
        "laravel": {
            "providers": [
                "Pinfort\\LaravelTopNav\\TopNavServiceProvider"
            ]
        }
    }
}
